<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".UI.BottomNavigationView.More">


        <RelativeLayout
            android:id="@+id/relativeLayout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/more_circle"
            android:padding="@dimen/_5sdp"
            app:layout_constraintBottom_toBottomOf="@+id/PaymentDetails"
            app:layout_constraintEnd_toEndOf="@+id/PaymentDetails"
            app:layout_constraintStart_toEndOf="@+id/PaymentDetails"
            app:layout_constraintTop_toTopOf="@+id/PaymentDetails">

            <ImageView
                android:layout_width="@dimen/_20sdp"
                android:layout_height="@dimen/_20sdp"
                android:src="@drawable/more_arraw" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/relativeLayout2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/more_circle"
            android:padding="@dimen/_5sdp"
            app:layout_constraintBottom_toBottomOf="@+id/MyOrder"
            app:layout_constraintEnd_toEndOf="@+id/MyOrder"
            app:layout_constraintStart_toEndOf="@+id/MyOrder"
            app:layout_constraintTop_toTopOf="@+id/MyOrder">

            <ImageView
                android:layout_width="@dimen/_20sdp"
                android:layout_height="@dimen/_20sdp"
                android:src="@drawable/more_arraw" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/more_circle"
            android:padding="@dimen/_5sdp"
            app:layout_constraintBottom_toBottomOf="@+id/NotifactionButton"
            app:layout_constraintEnd_toEndOf="@+id/NotifactionButton"
            app:layout_constraintStart_toEndOf="@+id/NotifactionButton"
            app:layout_constraintTop_toTopOf="@+id/NotifactionButton">

            <ImageView
                android:layout_width="@dimen/_20sdp"
                android:layout_height="@dimen/_20sdp"
                android:src="@drawable/more_arraw" />
        </RelativeLayout>


        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/more_circle"
            android:padding="@dimen/_5sdp"

            app:layout_constraintBottom_toBottomOf="@+id/InboxButton"
            app:layout_constraintEnd_toEndOf="@+id/InboxButton"
            app:layout_constraintStart_toEndOf="@+id/InboxButton"
            app:layout_constraintTop_toTopOf="@+id/InboxButton">

            <ImageView
                android:layout_width="@dimen/_20sdp"
                android:layout_height="@dimen/_20sdp"
                android:src="@drawable/more_arraw" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/more_circle"
            android:padding="@dimen/_5sdp"
            app:layout_constraintBottom_toBottomOf="@+id/AboutUsButton"
            app:layout_constraintEnd_toEndOf="@+id/AboutUsButton"
            app:layout_constraintStart_toEndOf="@+id/AboutUsButton"
            app:layout_constraintTop_toTopOf="@+id/AboutUsButton">

            <ImageView
                android:layout_width="@dimen/_20sdp"
                android:layout_height="@dimen/_20sdp"
                android:src="@drawable/more_arraw" />
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/more_circle"
            android:padding="@dimen/_5sdp"
            app:layout_constraintBottom_toBottomOf="@+id/MyBookMark"
            app:layout_constraintEnd_toEndOf="@+id/MyBookMark"
            app:layout_constraintStart_toEndOf="@+id/MyBookMark"
            app:layout_constraintTop_toTopOf="@+id/MyBookMark">

            <ImageView
                android:layout_width="@dimen/_20sdp"
                android:layout_height="@dimen/_20sdp"
                android:src="@drawable/more_arraw" />
        </RelativeLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/PaymentDetails"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:background="@drawable/more_card_bg"
            android:layout_marginTop="@dimen/_30sdp"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginRight="@dimen/_30sdp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_60sdp">

            <RelativeLayout
                android:id="@+id/Payment"
                android:background="@drawable/title_more_circle"
                android:layout_width="wrap_content"
                android:padding="@dimen/_7sdp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="@dimen/_15sdp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="@dimen/_18sdp"
                    android:layout_height="@dimen/_18sdp"
                    android:src="@drawable/ic_payment" />
            </RelativeLayout>

            <TextView
                app:layout_constraintStart_toEndOf="@id/Payment"
                app:layout_constraintTop_toTopOf="@id/Payment"
                app:layout_constraintBottom_toBottomOf="@id/Payment"
                android:layout_marginLeft="@dimen/_10sdp"
                android:text="Payment Details"
                android:fontFamily="@font/metropolisregular"
                android:textColor="#4A4B4D"
                android:textSize="@dimen/_12ssp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/MyOrder"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:background="@drawable/more_card_bg"
            android:layout_marginTop="@dimen/_20sdp"
            app:layout_constraintTop_toBottomOf="@+id/PaymentDetails"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginRight="@dimen/_30sdp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_60sdp">

            <RelativeLayout
                android:id="@+id/Oder"
                android:background="@drawable/title_more_circle"
                android:layout_width="wrap_content"
                android:padding="@dimen/_7sdp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="@dimen/_15sdp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:src="@drawable/shoppingbg"
                    android:layout_width="@dimen/_18sdp"
                    android:layout_height="@dimen/_18sdp" />
            </RelativeLayout>

            <TextView
                app:layout_constraintStart_toEndOf="@id/Oder"
                app:layout_constraintTop_toTopOf="@id/Oder"
                app:layout_constraintBottom_toBottomOf="@id/Oder"
                android:layout_marginLeft="@dimen/_10sdp"
                android:text="My Order"
                android:fontFamily="@font/metropolisregular"
                android:textColor="#4A4B4D"
                android:textSize="@dimen/_12ssp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/MyBookMark"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:background="@drawable/more_card_bg"
            android:layout_marginTop="@dimen/_20sdp"
            app:layout_constraintTop_toBottomOf="@+id/MyOrder"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginRight="@dimen/_30sdp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_60sdp">

            <RelativeLayout
                android:id="@+id/Oders"
                android:background="@drawable/title_more_circle"
                android:layout_width="wrap_content"
                android:padding="@dimen/_7sdp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="@dimen/_15sdp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:src="@drawable/ic_lovedeselected"
                    android:layout_width="@dimen/_18sdp"
                    android:layout_height="@dimen/_18sdp" />
            </RelativeLayout>

            <TextView
                app:layout_constraintStart_toEndOf="@id/Oders"
                app:layout_constraintTop_toTopOf="@id/Oders"
                app:layout_constraintBottom_toBottomOf="@id/Oders"
                android:layout_marginLeft="@dimen/_10sdp"
                android:text="My Favourites"
                android:fontFamily="@font/metropolisregular"
                android:textColor="#4A4B4D"
                android:textSize="@dimen/_12ssp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </androidx.constraintlayout.widget.ConstraintLayout>




        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/NotifactionButton"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:background="@drawable/more_card_bg"
            android:layout_marginTop="@dimen/_20sdp"
            app:layout_constraintTop_toBottomOf="@+id/MyBookMark"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginRight="@dimen/_30sdp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_60sdp">

            <RelativeLayout
                android:id="@+id/Notifaction"
                android:background="@drawable/title_more_circle"
                android:layout_width="wrap_content"
                android:padding="@dimen/_7sdp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="@dimen/_15sdp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="@dimen/_18sdp"
                    android:layout_height="@dimen/_18sdp"
                    android:src="@drawable/bell" />
            </RelativeLayout>

            <TextView
                app:layout_constraintStart_toEndOf="@id/Notifaction"
                app:layout_constraintTop_toTopOf="@id/Notifaction"
                app:layout_constraintBottom_toBottomOf="@id/Notifaction"
                android:layout_marginLeft="@dimen/_10sdp"
                android:text="Notification"
                android:fontFamily="@font/metropolisregular"
                android:textColor="#4A4B4D"
                android:textSize="@dimen/_12ssp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/InboxButton"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:background="@drawable/more_card_bg"
            android:layout_marginTop="@dimen/_20sdp"
            app:layout_constraintTop_toBottomOf="@+id/NotifactionButton"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginRight="@dimen/_30sdp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_60sdp">

            <RelativeLayout
                android:id="@+id/Inbox"
                android:background="@drawable/title_more_circle"
                android:layout_width="wrap_content"
                android:padding="@dimen/_7sdp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="@dimen/_15sdp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="@dimen/_18sdp"
                    android:layout_height="@dimen/_18sdp"
                    android:src="@drawable/inbox" />
            </RelativeLayout>

            <TextView
                app:layout_constraintStart_toEndOf="@id/Inbox"
                app:layout_constraintTop_toTopOf="@id/Inbox"
                app:layout_constraintBottom_toBottomOf="@id/Inbox"
                android:layout_marginLeft="@dimen/_10sdp"
                android:text="Inbox"
                android:fontFamily="@font/metropolisregular"
                android:textColor="#4A4B4D"
                android:textSize="@dimen/_12ssp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </androidx.constraintlayout.widget.ConstraintLayout>


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/AboutUsButton"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:background="@drawable/more_card_bg"
            android:layout_marginTop="@dimen/_20sdp"
            app:layout_constraintTop_toBottomOf="@+id/InboxButton"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_marginRight="@dimen/_30sdp"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_60sdp">

            <RelativeLayout
                android:id="@+id/AboutUs"
                android:background="@drawable/title_more_circle"
                android:layout_width="wrap_content"
                android:padding="@dimen/_7sdp"
                app:layout_constraintLeft_toLeftOf="parent"
                android:layout_marginLeft="@dimen/_15sdp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_height="wrap_content">

                <ImageView
                    android:layout_width="@dimen/_18sdp"
                    android:layout_height="@dimen/_18sdp"
                    android:src="@drawable/info" />
            </RelativeLayout>

            <TextView
                app:layout_constraintStart_toEndOf="@id/AboutUs"
                app:layout_constraintTop_toTopOf="@id/AboutUs"
                app:layout_constraintBottom_toBottomOf="@id/AboutUs"
                android:layout_marginLeft="@dimen/_10sdp"
                android:text="About Us"
                android:fontFamily="@font/metropolisregular"
                android:textColor="#4A4B4D"
                android:textSize="@dimen/_12ssp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>